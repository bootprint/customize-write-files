{
  "name": "customize-write-files",
  "version": "2.0.1",
  "description": "Post-processor that stores the result of a customize-run in a local directory",
  "repository": {
    "type": "git",
    "url": "https://github.com/bootprint/customize-write-files.git"
  },
  "author": {
